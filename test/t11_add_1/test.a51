	org 0x600
	dW  REG_SP,		0x7
	dW  REG_A,		0x6b
	dW  REG_B,		0x0
	dW  REG_PSW,	0x1
	dW  REG_PC,		0x59
	dw  REG_DPTR,	0x00
	dw 	CYCLE,		54

	dw  REG_R0,		0x03
	dw  REG_R1,		0x20

	dw  0x40,		0x77
	dw  0x41,		0x78
	dw  0x42,		0xA8
	dw  0x43,		0xA9
	dw  0x44,		0x8E
	dw  0x45,		0x8F
	dw  0x46,		0x6B
