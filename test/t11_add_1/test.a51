	org 0x0
start:
	; ADD A,Rn 
	MOV R0, #0x3
	MOV A, #0x74
	ADD A, R0
	MOV 0x40, A

	; ADDC A,Rn 
	MOV PSW, #0x80
	MOV R0, #0x3
	MOV A, #0x74
	ADDC A, R0
	MOV 0x41, A
	
	; ADD A,direct
	MOV 0x20, #0x34
	MOV A, #0x74
	ADD A, 0x20
	MOV 0x42, A
	
	; ADDC A,direct
	MOV PSW, #0x80
	MOV 0x20, #0x34
	MOV A, #0x74
	ADDC A, 0x20
	MOV 0x43, A

	; ADD A,#data 
	MOV A, #0x56
	ADD A, #0x38
	MOV 0x44, A
	
	; ADDC A,#data 
	MOV PSW, #0x80
	MOV A, #0x56
	ADDC A, #0x38
	MOV 0x45, A
	
	; ADD A,@Ri
	MOV 0x20, #0x34
	MOV R1, #0x20
	MOV A, #0x37
	ADD A, @R1
	MOV 0x46, A
	
	; ADDC A,@Ri
	MOV PSW, #0x80
	MOV 0x20, #0x34
	MOV R1, #0x20
	MOV A, #0x37
	ADDC A, @R1
	MOV 0x47, A
	
	; ADDC A,@Ri
	MOV PSW, #0x00
	MOV 0x20, #0x34
	MOV R1, #0x20
	MOV A, #0x37
	ADDC A, @R1
	MOV 0x48, A
		
	
	sjmp $
	
; for test
REG_SP		EQU	0x1000
REG_A		EQU	0x1001
REG_B		EQU	0x1002
REG_PSW		EQU	0x1003
